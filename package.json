{
  "name": "@restry/openclaw-general-channel",
  "version": "2.0.0",
  "type": "module",
  "description": "Generic WebSocket/Webhook channel plugin for OpenClaw",
  "license": "MIT",
  "files": [
    "index.ts",
    "src",
    "examples",
    "openclaw.plugin.json",
    "GENERIC_CHANNEL.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Restry/openclaw-general-channel.git"
  },
  "keywords": [
    "openclaw",
    "generic",
    "websocket",
    "webhook",
    "chatbot",
    "ai",
    "claude"
  ],
  "openclaw": {
    "extensions": [
      "./index.js"
    ],
    "channel": {
      "id": "openclaw-general-channel",
      "label": "Generic Channel",
      "selectionLabel": "Generic Channel (WebSocket/Webhook)",
      "docsPath": "/channels/openclaw-general-channel",
      "docsLabel": "openclaw-general-channel",
      "blurb": "Generic WebSocket/Webhook channel for H5 pages.",
      "aliases": [],
      "order": 100
    },
    "install": {
      "npmSpec": "@restry/openclaw-general-channel",
      "localPath": ".",
      "defaultChoice": "npm"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "openclaw": "2026.1.29",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.29"
  }
}
